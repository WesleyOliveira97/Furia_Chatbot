{
  "name": "furia_chatbot",
  "version": "1.0.0",
  "description": "A modern, responsive chatbot interface built with HTML, CSS, and JavaScript.",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "server": "nodemon server.js",
    "client": "cd client && react-scripts start",
    "build": "cd client && react-scripts build",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test": "cd client && react-scripts test",
    "eject": "cd client && react-scripts eject"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WesleyOliveira97/Furia_Chatbot.git"
  },
  "keywords": [
    "chatbot",
    "nodejs",
    "express",
    "html",
    "css"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/WesleyOliveira97/Furia_Chatbot/issues"
  },
  "homepage": "https://github.com/WesleyOliveira97/Furia_Chatbot#readme",
  "dependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-scripts": "^5.0.1"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "nodemon": "^3.0.1"
